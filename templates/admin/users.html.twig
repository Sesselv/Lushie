{% extends 'baseAdmin.html.twig' %} {% block javascripts %}
{{ importmap(["app"]) }}
{% endblock %} {% block body %}
<section class="sec-users">
    <div class="container">
        <h1>Liste des utilisateurs</h1>
        <ul>
            {% for user in users %}
            <li>
                <a href="{{ path('admin_user_show', { id: user.id }) }}">
